	<queryString>
		<![CDATA[SELECT
	tp.id AS id,
	tp.data_saida AS data_saida,
	tp.motivo,
	re.nome	AS responsavel_envio,
	rr.nome	AS responsavel_recebimento,
	sd.nome	AS setor_destino,
	so.nome	AS setor_origem,
	o.descricao AS orgao_nome,
	i.id AS item_id,
	i.nome AS item_nome,
	p.numero AS numero_tombamento,
	p.modelo,
	p.numero_serie AS numero_serie,
	ed.nome AS estado_depreciacao
FROM almoxarifado.transferencia_patrimonio tp
INNER JOIN almoxarifado.usuario re ON (tp.responsavelenvio_id = re.id)
INNER JOIN almoxarifado.usuario rr ON (tp.responsavelrecebimento_id = rr.id)
INNER JOIN almoxarifado.departamento so ON (tp.setororigem_id = so.id)
INNER JOIN almoxarifado.departamento sd ON (tp.setordestino_id = sd.id)
INNER JOIN almoxarifado.unidade un ON (un.id = so.unidade_id)
INNER JOIN almoxarifado.orgao o ON (o.id = un.orgao_id)
INNER JOIN almoxarifado.transferencia_bens tb ON (tb.transferencia_id = tp.id)
INNER JOIN almoxarifado.patrimonio p ON (p.id = tb.patrimonio_id)
INNER JOIN almoxarifado.item_entrada ie ON (ie.id = p.item_entrada_id)
INNER JOIN almoxarifado.item i ON (i.id = ie.item_id)
INNER JOIN almoxarifado.estado_depreciacao ed ON (ed.id = p.estado_depreciacao_id)
WHERE tp.id = $P{transferencia_id}]]>
	</queryString>
