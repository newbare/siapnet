	<queryString>
		<![CDATA[SELECT p.id,p.numero as tombo,  data_tombamento,
       d.sigla as setor, p.localizacao, ed.nome as estado,
       i.nome, marca.nome as marca,modelo, numero_serie,
       p.observacao, garantia, g.nome as grupo, almox.nome as almoxarifado
  FROM almoxarifado.patrimonio p
  INNER JOIN almoxarifado.item_entrada ie ON ie.id = p.item_entrada_id
  INNER JOIN almoxarifado.item i ON ie.item_id = i.id
  INNER JOIN almoxarifado.nota_entrada ne ON ne.id = ie.nota_entrada_id
  LEFT JOIN almoxarifado.marca marca ON marca.id = p.marca_id
  LEFT JOIN almoxarifado.estado_depreciacao ed ON ed.id = p.estado_depreciacao_id
  LEFT JOIN almoxarifado.departamento d ON d.id = p.departamento_id
  LEFT JOIN almoxarifado.grupo g ON g.id = i.grupo_id
  INNER JOIN almoxarifado.almoxarifado almox ON almox.id = ne.almoxarifado_id]]>
